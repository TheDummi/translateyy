{
	"name": "translateyy",
	"version": "1.0.1",
	"description": "A translate package that combines multiple API's and packages.",
	"main": "./dist/main.cjs",
	"module": "./dist/main.mjs",
	"exports": {
		"require": {
			"types": "./dist/main.d.cts",
			"default": "./dist/main.cjs"
		},
		"import": {
			"types": "./dist/main.d.mts",
			"default": "./dist/main.mjs"
		}
	},
	"scripts": {
		"build": "tsc && npx prettier --write .",
		"dev": "pkgroll --watch --tsconfig=tsconfig.json --clean-dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TheDummi/translateyy.git"
	},
	"keywords": [
		"translate",
		"free"
	],
	"author": "Dummi",
	"type": "module",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/TheDummi/translateyy/issues"
	},
	"homepage": "https://github.com/TheDummi/translateyy#readme",
	"dependencies": {},
	"devDependencies": {
		"prettier": "^2.8.8",
		"typescript": "^5.1.3",
		"pkgroll": "2.4.2"
	}
}
